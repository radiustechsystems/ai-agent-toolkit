{
  "name": "@radiustechsystems/ai-agent-toolkit",
  "version": "0.1.0",
  "description": "TypeScript AI Agent Toolkit for integrating AI agents with the Radius Platform for smart contracts, automation, and payments",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/radiustechsystems/ai-agent-toolkit.git"
  },
  "type": "module",
  "scripts": {
    "build": "pnpm build:all",
    "build:libs": "pnpm turbo --filter \"./packages/**\" build",
    "build:libs:prod": "cross-env NODE_ENV=production pnpm build:libs",
    "build:umbrella": "tsup",
    "build:all": "pnpm build:libs && pnpm build:umbrella",
    "build:all:prod": "cross-env NODE_ENV=production pnpm build:all",
    "prepublishOnly": "pnpm build:all:prod",
    "clean:dist": "pnpm rimraf \"**/dist\" --glob",
    "clean:node_modules": "pnpm rimraf \"**/node_modules\" --glob",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "pnpm turbo test"
  },
  "dependencies": {
    "@radiustechsystems/ai-agent-core": "^0.1.0",
    "@radiustechsystems/ai-agent-wallet": "^0.1.0",
    "@radiustechsystems/ai-agent-adapter-vercel-ai": "^0.1.0",
    "@radiustechsystems/ai-agent-adapter-langchain": "^0.1.0",
    "@radiustechsystems/ai-agent-adapter-model-context-protocol": "^0.1.0",
    "@radiustechsystems/ai-agent-plugin-balancer": "^0.1.0",
    "@radiustechsystems/ai-agent-plugin-contracts": "^0.1.0",
    "@radiustechsystems/ai-agent-plugin-crypto": "^0.1.0",
    "@radiustechsystems/ai-agent-plugin-erc20": "^0.1.0",
    "@radiustechsystems/ai-agent-plugin-uniswap": "^0.1.0"
  },
  "devDependencies": {
    "@types/node": "22.7.4",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "cross-env": "7.0.3",
    "eslint": "8.56.0",
    "rimraf": "6.0.1",
    "tsup": "8.3.5",
    "turbo": "2.3.1",
    "typescript": "5.3.3",
    "vitest": "2.1.5"
  },
  "engines": {
    "node": ">=20.12.2 <23",
    "pnpm": ">=9"
  },
  "license": "MIT",
  "packageManager": "pnpm@9.14.2",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "keywords": [
    "radius",
    "evm",
    "automation",
    "payment",
    "llm",
    "agent",
    "agentic",
    "toolkit",
    "ai"
  ],
  "homepage": "https://github.com/radiustechsystems/ai-agent-toolkit/tree/main/typescript#readme",
  "bugs": {
    "url": "https://github.com/radiustechsystems/ai-agent-toolkit/issues"
  },
  "publishConfig": {
    "access": "public"
  }
}
